<Window x:Class="Cliente.VentanaRegistroUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cliente"
        mc:Ignorable="d"
        Title="VentanaRegistroUsuario" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbNombreUsuario" HorizontalAlignment="Center" Margin="0,154,0,0" TextWrapping="Wrap" Text="Nombre de usuario" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbCorreo" HorizontalAlignment="Center" Margin="0,241,0,0" TextWrapping="Wrap" Text="Correo electrónico" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbContrasenia" HorizontalAlignment="Center" Margin="0,199,0,0" TextWrapping="Wrap" Text="Contraseña" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnSiguiente" Content="Siguiente" HorizontalAlignment="Center" Margin="0,305,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.168,0.603" Click="btnSiguiente_Click"/>
        <Button x:Name="btnCancelar" Content="Cancelar" HorizontalAlignment="Center" Margin="0,351,0,0" VerticalAlignment="Top" Click="btnCancelar_Click"/>

    </Grid>
</Window>
